    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.1",
    "@vitest/coverage-v8": "3.2.2",
    "@vitest/ui": "3.2.2",
    "eslint": "9.28.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.2.0",
    "jsdom": "26.1.0",
