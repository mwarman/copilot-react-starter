{
  "name": "copilot-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc -b && vite build",
    "clean": "rimraf dist coverage",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "deploy": "npm run build && cd infrastructure && npm install && npm run deploy",
    "cdk": "cd infrastructure && npm run cdk"
  },
  "dependencies": {
